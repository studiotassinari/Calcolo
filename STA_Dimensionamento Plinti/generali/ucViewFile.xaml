<UserControl x:Class="STA_Dimensionamento_Plinti.generali.ucViewFile"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:myPropControlGen="clr-namespace:STA_Dimensionamento_Plinti.generali"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Name="titolo" Height="30"></RowDefinition>
            <RowDefinition Name="cont"></RowDefinition>
        </Grid.RowDefinitions>
        <Label Name="laFilePath" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" ></Label>
        <FlowDocumentReader Grid.Row="1" Name="FlowDocReader" FontFamily="Courier New" ViewingMode="Scroll"></FlowDocumentReader>
    </Grid>
</UserControl>
