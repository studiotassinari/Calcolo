<ribbon:RibbonTab x:Class="STA.Prefabbricato.Interfaccia.Moduli.GestioneTraveIsostatica.Views.ModuloGestioneTraveIsostaticaMainRibbonTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:ribbon="clr-namespace:Microsoft.Windows.Controls.Ribbon;assembly=RibbonControlsLibrary"
                  Header="Main"
             mc:Ignorable="d" 
             >
    <ribbon:RibbonGroup Header="Proprietà Trave Isostatica">
        <ribbon:RibbonTextBox Text="{Binding NomeTrave}"></ribbon:RibbonTextBox>
        <ribbon:RibbonSeparator></ribbon:RibbonSeparator>
    </ribbon:RibbonGroup>
    <ribbon:RibbonGroup Header="Gestione File"></ribbon:RibbonGroup>
    
</ribbon:RibbonTab>
