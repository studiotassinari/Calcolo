<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d">
	<Color x:Key="Colore1">#FF022B3D</Color>
	<!-- Resource dictionary entries should be defined here. -->
	<ControlTemplate x:Key="LabelTemplateRisultatiSuperficiali" TargetType="{x:Type Label}">
			<TextBlock Margin="2" TextWrapping="Wrap" Text="" VerticalAlignment="Stretch" d:LayoutOverrides="Width" FontFamily="Lucida Sans Unicode" FontSize="14.667" Background="#A3FFFFFF"/>
	</ControlTemplate>
	<Style x:Key="TextBlockStyleCombinazioni" TargetType="{x:Type TextBlock}">
		<Setter Property="TextWrapping" Value="NoWrap"/>
		<Setter Property="TextTrimming" Value="None"/>
		<Setter Property="Margin" Value="1"/>
		<Setter Property="Background" Value="#96FFFFFF"/>
		<Setter Property="Text" Value=""/>
		<Setter Property="TextAlignment" Value="Center"/>
		<Setter Property="VerticalAlignment" Value="Center"/>
	</Style>
</ResourceDictionary>